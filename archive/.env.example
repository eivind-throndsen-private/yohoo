# Yohoo Project - Environment Configuration Template
# Copy this file to .env and fill in your values

# Chrome History Database Path (macOS default)
CHROME_HISTORY_PATH=~/Library/Application Support/Google/Chrome/Default/History

# Bookmark File Path
BOOKMARKS_FILE=input/bookmarks_08.12.2025.html

# Output Paths
OUTPUT_HTML=yohoo.html
BOOKMARKS_JSON=data/bookmarks.json
HISTORY_JSON=data/history.json
MERGED_JSON=data/merged.json

# History Analysis Settings
HISTORY_DAYS=90
MIN_VISITS=3

# Logging Level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Debug Mode
DEBUG=false
